<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView27"
        android:layout_width="126dp"
        android:layout_height="35dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="156dp"
        android:layout_marginRight="156dp"
        android:text="Accelerometer" />

    <TextView
        android:id="@+id/accX"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView27"
        android:layout_marginBottom="-88dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/accY"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginBottom="627dp"
        android:layout_toEndOf="@+id/accX"
        android:layout_toRightOf="@+id/accX"
        android:text="TextView" />

    <TextView
        android:id="@+id/accZ"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="627dp"
        android:layout_toEndOf="@+id/accY"
        android:layout_toRightOf="@+id/accY"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="126dp"
        android:layout_height="32dp"
        android:layout_below="@+id/textView27"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="143dp"
        android:layout_marginRight="143dp"
        android:text="Gyroscope" />

    <TextView
        android:id="@+id/GyroY"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView32"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="156dp"
        android:layout_marginRight="156dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/GyroX"
        android:layout_width="92dp"
        android:layout_height="19dp"
        android:layout_below="@+id/accX"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="255dp"
        android:layout_marginEnd="292dp"
        android:layout_marginRight="292dp"
        android:layout_marginBottom="422dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/GyroZ"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="444dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="126dp"
        android:layout_height="32dp"
        android:layout_below="@+id/GyroY"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="140dp"
        android:layout_marginRight="140dp"
        android:text="Pressure" />

    <TextView
        android:id="@+id/pressureX"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView36"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="154dp"
        android:layout_marginLeft="154dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="152dp"
        android:layout_marginRight="152dp"
        android:text="TextView" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/pressureX"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="285dp"
        android:layout_marginRight="285dp"
        android:layout_marginBottom="112dp"
        android:text="Record" />

    <Button
        android:id="@+id/stopBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/pressureX"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Stop" />

</RelativeLayout>